local library = loadstring(game:HttpGet('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3'))()

local w = library:CreateWindow("UGCSCRIPT")
local a = w:CreateFolder("RECOLECTAR")
local b = w:CreateFolder("NPC=R")
local c = w:CreateFolder("CHECKPOINTS")
local d = w:CreateFolder("TERMINAR")

local teleporting = false
local teleportObjectName = nil
local teleportCoroutine = nil

local function safeTeleport(cframe)
    local player = game.Players.LocalPlayer
    if player and player.Character and player.Character.PrimaryPart then
        player.Character:SetPrimaryPartCFrame(cframe)
    end
end

local function teleportToObject(objectName)
    local object = game.Workspace.Pickups:FindFirstChild(objectName)
    if object and object.CFrame then
        safeTeleport(object.CFrame)
    else
        warn("Objeto no encontrado: " .. tostring(objectName))
    end
end

-- Uso de coroutine para no bloquear el hilo principal
local function toggleTeleporting(objectName)
    teleportObjectName = objectName
    teleporting = not teleporting

    if teleporting then
        teleportCoroutine = coroutine.create(function()
            while teleporting do
                teleportToObject(teleportObjectName)
                wait(0.1)
            end
        end)
        coroutine.resume(teleportCoroutine)
    end
end

a:Button("INICIAR TELETRANSPORTE", function()
    local objectNames = {"Default"}
    for _, objectName in ipairs(objectNames) do
        toggleTeleporting(objectName)
    end
    game:GetService("StarterGui"):SetCore("SendNotification", { 
        Title = "TELETRANSPORTE ACTIVADO",
        Text = "Iniciando teletransporte continuo para el objeto Default.",
        Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150",
        Duration = 3
    })
end)

b:Button("INICIAR", function()
    local locations = {
        workspace.PickupSlots.RevealPickupsSteveEagle:FindFirstChild("Default"),
        workspace.PickupSlots.RevealPickupsSteveGecko:FindFirstChild("Default"),
        workspace.PickupSlots.RevealPickupsSteveMantisShrimp:FindFirstChild("Default"),
        workspace.PickupSlots.RevealPickupsSteveSnowLeopard:FindFirstChild("Default"),
    }
    for _, location in ipairs(locations) do
        if location and location.CFrame then
            safeTeleport(location.CFrame)
            wait(3)
        else
            warn("Ubicaci贸n no encontrada.")
        end
    end
    game:GetService("StarterGui"):SetCore("SendNotification", { 
        Title = "TELETRANSPORTE FINALIZADO",
        Text = "Has sido teletransportado a todas las ubicaciones.",
        Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150",
        Duration = 3
    })
end)

c:Button("INICIAR", function()
    local locations = {
        workspace.Stickers.Cheetah.CheckpointNoSticker00A.Touch,
        workspace.ActiveStickers.d6001.Touch,
        workspace.Stickers.Cheetah.CheckpointNoSticker01A.Touch,
        workspace.Stickers.Cheetah.CheckpointNoSticker01B.Touch,
        workspace.ActiveStickers.d6002.Touch,
        workspace.Stickers.Cheetah.CheckpointNoSticker02A.Touch,
        workspace.ActiveStickers.d6003.Touch,
        workspace.ActiveStickers.d6004.Touch,
        workspace.ActiveStickers.d6005.Touch,
        workspace.ActiveStickers.d6006.Touch,
        workspace.Stickers.SnowLeopard.CheckpointNoSticker06A.Touch,
        workspace.Stickers.SnowLeopard.CheckpointNoSticker06B.Touch,
        workspace.ActiveStickers.d6007.Touch,
        workspace.Stickers.SnowLeopard.CheckpointNoSticker07A.Touch,
        workspace.ActiveStickers.d6008.Touch,
        workspace.Stickers.SnowLeopard.CheckpointNoSticker08A.Touch,
        workspace.ActiveStickers.d6009.Touch,
        workspace.ActiveStickers.d6010.Touch,
        workspace.Stickers.SnowLeopard.CheckpointNoSticker09A.Touch,
        workspace.ActiveStickers.d6011.Touch,
        workspace.ActiveStickers.d6012.Touch,
        workspace.Stickers.Eagle.CheckpointNoSticker12A,
        workspace.ActiveStickers.d6013.Touch,
        workspace.Stickers.Eagle.CheckpointNoSticker12B.Touch,
        workspace.Stickers.Eagle.CheckpointNoSticker13A.Touch,
        workspace.ActiveStickers.d6014.Touch,
        workspace.Stickers.Eagle.CheckpointNoSticker14A.Touch,
        workspace.ActiveStickers.d6015.Touch,
    }
    -- Eliminar duplicados creando una tabla de vistos
    local seen = {}
    for _, location in ipairs(locations) do
        if location and location.CFrame and not seen[location] then
            seen[location] = true
            safeTeleport(location.CFrame)
            wait(0.5)
        else
            warn("Ubicaci贸n no encontrada o duplicada.")
        end
    end
    game:GetService("StarterGui"):SetCore("SendNotification", { 
        Title = "TELEPOR FINALIZADO",
        Text = "Has sido teletransportado a todas las ubicaciones.",
        Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150",
        Duration = 3
    })
end)

d:Button("INICIAR", function()
    local player = game:GetService("Players").LocalPlayer
    local destination = game.Workspace.Utility.Teleporters.Portal_to_Wc.Portal["Canvas Clouds"]:FindFirstChild("Cylinder")
    if destination and destination.CFrame then
        safeTeleport(destination.CFrame)
        game:GetService("StarterGui"):SetCore("SendNotification", { 
            Title = "TELETRANSPORTADO",
            Text = "Has sido teletransportado a la ubicaci贸n deseada.",
            Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150",
            Duration = 3
        })
    else
        game:GetService("StarterGui"):SetCore("SendNotification", { 
            Title = "ERROR",
            Text = "No se encontr贸 el destino.",
            Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150",
            Duration = 3
        })
    end
end)
