local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3')))()

local w = library:CreateWindow("UGCSCRIPT")

local b = w:CreateFolder("RECOLECTAR ")
local c = w:CreateFolder("NPC=R ")
local d = w:CreateFolder("CHECKPOINTS ")
local e = w:CreateFolder("TERMINAR ")

-- Variable para controlar el teletransporte continuo
local teleporting = false
local teleportObjectName = nil

-- Funci贸n para teleportar al jugador a un objeto
local function teleportToObject(objectName)
    local object = game.Workspace.Pickups:FindFirstChild(objectName)
    if object then
        game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(object.CFrame)
    else
        warn("Objeto no encontrado: " .. objectName)
    end
end

-- Funci贸n para iniciar o detener el teletransporte continuo
local function toggleTeleporting(objectName)
    teleportObjectName = objectName
    teleporting = not teleporting

    if teleporting then
        while teleporting do
            teleportToObject(teleportObjectName)
            wait(0.1) -- Espera menos de medio segundo entre teletransportes
        end
    end
end

b:Button("INICIAR TELETRANSPORTE", function()
    local objectNames = {"Default"}
    for _, objectName in ipairs(objectNames) do
        toggleTeleporting(objectName)
    end
    
    game:GetService("StarterGui"):SetCore("SendNotification", { 
        Title = "TELETRANSPORTE ACTIVADO";
        Text = "Iniciando teletransporte continuo para el objeto Default.";
        Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150"
    })
end)

c:Button("INICIAR", function()
    local locations = {
        workspace.PickupSlots.RevealPickupsSteveEagle:FindFirstChild("Default"),
        workspace.PickupSlots.RevealPickupsSteveGecko:FindFirstChild("Default"),
        workspace.PickupSlots.RevealPickupsSteveMantisShrimp:FindFirstChild("Default"),
        workspace.PickupSlots.RevealPickupsSteveSnowLeopard:FindFirstChild("Default"),
    }

    for _, location in ipairs(locations) do
        if location then
            game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(location.CFrame)
            wait(3) -- Espera menos de medio segundo entre teleportaciones
        else
            warn("Ubicaci贸n no encontrada.")
        end
    end

    game:GetService("StarterGui"):SetCore("SendNotification", { 
        Title = "TELETRANSPORTE FINALIZADO";
        Text = "Has sido teletransportado a todas las ubicaciones.";
        Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150"
    })
end)

d:Button("INICIAR", function()
    -- Lista de ubicaciones donde buscar el objeto Touch
    local targets = {
        workspace.Stickers.Cheetah.CheckpointNoSticker00A,
        workspace.ActiveStickers.d6001,
        workspace.Stickers.Cheetah.CheckpointNoSticker01A,
        workspace.Stickers.Cheetah.CheckpointNoSticker01B,
        workspace.ActiveStickers.d6002,
        workspace.Stickers.Cheetah.CheckpointNoSticker02A,
        workspace.ActiveStickers.d6003,
        workspace.ActiveStickers.d6004,
        workspace.ActiveStickers.d6005,
        workspace.ActiveStickers.d6006,
        workspace.Stickers.SnowLeopard.CheckpointNoSticker06A,
        workspace.Stickers.SnowLeopard.CheckpointNoSticker06B,
        workspace.ActiveStickers.d6007,
        workspace.Stickers.SnowLeopard.CheckpointNoSticker07A,
        workspace.ActiveStickers.d6008,
        workspace.Stickers.SnowLeopard.CheckpointNoSticker08A,
        workspace.ActiveStickers.d6009,
        workspace.Stickers.SnowLeopard.CheckpointNoSticker09A,
        workspace.ActiveStickers.d6010,
        workspace.ActiveStickers.d6011,
        workspace.ActiveStickers.d6012,
        workspace.Stickers.Eagle.CheckpointNoSticker12A,
        workspace.Stickers.Eagle.CheckpointNoSticker12B,
        workspace.ActiveStickers.d6013,
        workspace.Stickers.Eagle.CheckpointNoSticker13A,
        workspace.Stickers.Eagle.CheckpointNoSticker14A,
        workspace.ActiveStickers.d6014,
        workspace.ActiveStickers.d6015,
    }

    local function getTargetPart(obj)
        if not obj then return nil end
        local touch = obj:FindFirstChild("Touch")
        if touch then
            if touch:IsA("BasePart") then
                return touch
            elseif touch:IsA("TouchTransmitter") or touch:IsA("TouchInterest") then
                if touch.Parent and touch.Parent:IsA("BasePart") then
                    return touch.Parent
                end
            end
        end
        return nil
    end

    local character = game.Players.LocalPlayer.Character
    if not character or not character.PrimaryPart then
        warn("Tu personaje no est谩 cargado o falta PrimaryPart. Intenta nuevamente.")
        return
    end

    for i, obj in ipairs(targets) do
        local part = getTargetPart(obj)
        if part then
            character:SetPrimaryPartCFrame(part.CFrame)
            wait(0.5)
        else
            warn(("Ubicaci贸n %d no encontrada o sin objeto teletransportable."):format(i))
        end
    end

    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "TELETRANSPORTE FINALIZADO",
        Text = "Has sido teletransportado a todas las ubicaciones.",
        Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150"
    })
end)

e:Button("INICIAR", function()
    local player = game:GetService("Players").LocalPlayer
    local destination = game.Workspace.Utility.Teleporters.Portal_to_Wc.Portal["Canvas Clouds"]:FindFirstChild("Cylinder") -- Cambia "Destino" por el nombre de tu objeto o destino

    if destination then
        player.Character:SetPrimaryPartCFrame(destination.CFrame) -- Teletransporta al jugador
        game:GetService("StarterGui"):SetCore("SendNotification", { 
            Title = "TELETRANSPORTADO";
            Text = "Has sido teletransportado a la ubicaci贸n deseada.";
            Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150"
        })
    else
        game:GetService("StarterGui"):SetCore("SendNotification", { 
            Title = "ERROR";
            Text = "No se encontr贸 el destino.";
            Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150"
        })
    end
end)
